<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <packageSources>
        <clear />
        <add key="PowerToysCDPX_Feed" value="https://github-private.pkgs.visualstudio.com/microsoft/_packaging/PowerToysCDPX_Feed/nuget/v3/index.json" />
        <add key="Labs" value="https://pkgs.dev.azure.com/dotnet/CommunityToolkit/_packaging/CommunityToolkit-Labs/nuget/v3/index.json" /> <!-- For the dotnet community toolkit labs feed -->
    </packageSources>
    <disabledPackageSources>
        <clear />
    </disabledPackageSources>

    <packageSourceMapping>
        <packageSource key="PowerToysCDPX_Feed">
            <package pattern="*" />
        </packageSource>
        <packageSource key="Labs">
            <package pattern="CommunityToolkit.Labs.*" />
        </packageSource>
    </packageSourceMapping>
</configuration>
